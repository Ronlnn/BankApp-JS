{
    "useTabs": true,
    "tabWidth": 4,
    "singleQuote": true,
    "semi": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@/core/(.*)$",
        "^@/core/component/(.*)$",
        "^@/components/(.*)$",
        "^@/components/layout/(.*)$",
        "^@/components/screens/(.*)$",
        "^@/components/ui/(.*)/$",
        "^@/styles/(.*)$",
        "^@/config/(.*)$",
        "^@/utils/(.*)$",
        "^@/services/(.*)$",
        "^@/store/(.*)$",
        "^@/api/(.*)$",
        "^.+\\.(template\\.html|module\\.scss)$",
        "^../(.*)",
        "^./(.*)"
