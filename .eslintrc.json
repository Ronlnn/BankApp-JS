{
	"env": {
	  "browser": true,
	  "es2021": true,
	  "node": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
	  "ecmaVersion": "latest",
	  "sourceType": "module"
	},
	"rules": {
		"no-tabs": 0
	},
	"settings": {
		"import/resolver":	{
			"alias":	{
				"map":	[["@",	"./src"]]
			},
			"extensions":	[".js"]
		}
	}
  }
  